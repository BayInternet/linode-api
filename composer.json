{
    "name": "arodygin/linode-api",
    "description": "Linode API Client Library for PHP",
    "type": "library",
    "keywords": ["api", "linode"],
    "homepage": "https://github.com/arodygin/php-linode-api",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/validator": "^2.8|^3.0",
        "arodygin/php-dictionary": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "leedavis81/altr-ego": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Linode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Linode\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
